year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{emisc}: Evan Odell's various functions",
         author = c(person("Evan", "Odell")),
         year = year,
         note = note,
         doi = "10.5281/zenodo.825637",
         url = "https://github.com/evanodell/emisc")


